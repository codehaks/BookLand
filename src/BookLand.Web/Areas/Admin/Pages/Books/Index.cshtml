@page
@model BookLand.Web.Areas.Admin.Pages.Books.IndexModel
@{
}

<h1>Books</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Title</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var book in Model.BookList)
        {
            <tr>
                <td>@book.Title</td>
            </tr>
        }
    </tbody>
</table>