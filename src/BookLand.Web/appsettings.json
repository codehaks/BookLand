{
  "serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  //"Logging": {
  //  "LogLevel": {
